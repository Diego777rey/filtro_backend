type Cajero{
    id: ID!
    codigoCajero: String!
    turno: String!
    fechaIngreso: String!
    estado: String!
    caja: Caja!
    persona: Persona!
}
input InputCajero{
    codigoCajero: String!
    turno: String!
    fechaIngreso: String!
    estado: String!
    cajaId: ID!
    personaId: ID!
}
type PaginadorCajero {
    items: [Cajero!]!
    totalItems: Int!
    totalPages: Int!
    currentPage: Int!
    itemsCount: Int!
}