type Categoria{
    id: ID!
    nombre: String!
    categoriaEstado: String!
}
input InputCategoria {
    nombre: String!
    categoriaEstado: String!
    productoIds: [ID!]
}
type PaginadorCategoria {
    items: [Categoria!]!
    totalItems: Int!
    totalPages: Int!
    currentPage: Int!
    itemsCount: Int!
}