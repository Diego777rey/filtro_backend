type Inventario{
    id: ID!
    fechaMovimiento: String!
    tipoMovimiento: String!
    cantidad: Int!
    stockAnterior: Int!
    stockNuevo: Int!
    motivo: String!
    estadoInventario: EstadoInventario!
    producto: Producto!
    depositero: Depositero!
    ventaDetalle: VentaDetalle
}
input InputInventario{
    fechaMovimiento: String!
    tipoMovimiento: String!
    cantidad: Int!
    stockAnterior: Int!
    stockNuevo: Int!
    motivo: String!
    estadoInventario: EstadoInventario!
    productoId: ID!
    depositeroId: ID!
    ventaDetalleId: ID
}
type PaginadorInventario{
    items: [Inventario!]!
    totalItems: Int!
    totalPages: Int!
    currentPage: Int!
    itemsCount: Int!
}
enum EstadoInventario {
    FALTA
    SOBRA
    OK
}